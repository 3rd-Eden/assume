{
  "name": "assume",
  "version": "0.0.10",
  "description": "Expect-like assertions that works seamlessly in node and browsers",
  "main": "index.js",
  "scripts": {
    "browserify-tests": "browserify test/test.js -o test/phantom.js --debug",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha -- --reporter spec --ui bdd test/test.js",
    "phantom": "mocha-phantomjs test/phantom.html",
    "prepublish": "browserify index.js -o dist/assume.js --standalone assume",
    "node": "mocha --reporter spec --ui bdd test/test.js",
    "test": "npm run-script browserify-tests && npm run-script phantom",
    "test-travis": "istanbul cover node_modules/.bin/_mocha --report lcovonly -- --reporter spec --ui bdd test/test.js",
    "watch": "mocha --watch --reporter spec --ui bdd test/test.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/3rd-Eden/assume.git"
  },
  "bugs": {
    "url": "https://github.com/3rd-Eden/assume/issues"
  },
  "keywords": [
    "expect",
    "assert",
    "assertion",
    "asserts",
    "assume",
    "testing",
    "test",
    "tests",
    "spec",
    "should"
  ],
  "author": "Arnout Kazemier",
  "license": "MIT",
  "devDependencies": {
    "browserify": "7.0.x",
    "istanbul": "0.3.x",
    "mocha": "2.0.x",
    "mocha-phantomjs": "3.5.x",
    "phantomjs": "1.9.x",
    "pre-commit": "0.0.x"
  },
  "dependencies": {
    "deep-eql": "0.1.x",
    "is-node": "0.0.x",
    "pathval": "0.1.x",
    "prettify-error": "0.1.x"
  },
  "testling": {
    "harness": "mocha-bdd",
    "files": "test/*.js",
    "browsers": [
      "ie/6..latest",
      "chrome/latest",
      "firefox/latest"
    ]
  }
}
