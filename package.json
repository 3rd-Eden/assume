{
  "name": "assume",
  "version": "0.0.4",
  "description": "Expect-like assertions that works seamlessly in node and browsers",
  "main": "index.js",
  "scripts": {
    "test": "mocha $(find test -name '*.test.js')"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/3rd-Eden/assume.git"
  },
  "bugs": {
    "url": "https://github.com/3rd-Eden/assume/issues"
  },
  "keywords": [
    "expect",
    "assert",
    "assertion",
    "asserts",
    "assume",
    "testing",
    "test",
    "tests",
    "spec",
    "should"
  ],
  "author": "Arnout Kazemier",
  "license": "MIT",
  "devDependencies": {
    "pre-commit": "0.0.x",
    "mocha": "1.19.x"
  },
  "dependencies": {
    "backtrace": "0.0.x",
    "deep-eql": "0.1.x",
    "pathval": "0.1.x"
  },
  "testling": {
    "harness": "mocha-bdd",
    "files": "test/*.js",
    "browsers": [
      "ie/6..latest",
      "chrome/latest",
      "firefox/latest"
    ]
  }
}
